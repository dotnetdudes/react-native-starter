    "@react-navigation/native": "^6.1.6",
    "@react-navigation/native-stack": "^6.9.12",
    "@reduxjs/toolkit": "^1.9.5",
    "apisauce": "^3.0.0",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.0",
    "i18next": "^22.5.0",
    "jwt-decode": "^3.1.2",
    "react": "18.2.0",
    "react-i18next": "^12.3.1",
    "react-native": "^0.71.8",
    "react-native-app-auth": "^6.4.3",
    "react-native-gesture-handler": "^2.10.1",
