    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "type-check": "tsc",
    "generate": "plop --plopfile internals/generators/plopfile.ts"
  },
  "dependencies": {
    "@mdi/font": "^7.2.96",
    "@react-native-async-storage/async-storage": "^1.18.1",
    "@react-native-masked-view/masked-view": "^0.2.9",
