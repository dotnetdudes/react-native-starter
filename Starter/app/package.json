    "@formatjs/intl-relativetimeformat": "^11.2.7",
    "@mdi/font": "^7.3.67",
    "@os-team/i18next-react-native-language-detector": "^1.0.30",
    "@react-native-async-storage/async-storage": "^1.19.4",
    "@react-native-masked-view/masked-view": "^0.3.0",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.17",
    "@reduxjs/toolkit": "^1.9.7",
    "apisauce": "^3.0.0",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.0",
    "i18next": "^23.6.0",
    "i18next-async-storage-backend": "^1.0.2",
    "i18next-async-storage-backend2": "^2.0.3",
    "i18next-chained-backend": "^4.5.0",
    "jwt-decode": "^4.0.0",
    "react": "18.2.0",
    "react-i18next": "^13.3.1",
    "react-native": "^0.72.6",
    "react-native-app-auth": "^7.1.0",
    "react-native-gesture-handler": "^2.13.4",
    "react-native-keychain": "^8.1.2",
    "react-native-paper": "^5.11.1",
    "react-native-reanimated": "^3.5.4",
    "react-native-safe-area-context": "^4.7.4",
    "react-native-screens": "^3.27.0",
    "react-native-vector-icons": "^10.0.1",
    "react-redux": "8.1.3",
    "redux": "^4.2.1",
    "redux-injectors": "^2.1.0",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.2.3"
